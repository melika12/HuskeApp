<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HuskeApp.Views.login">
    
    <StackLayout>
        <Entry x:Name="username" Placeholder="Username" HorizontalTextAlignment="Center" FontSize="36"></Entry>
        <Entry x:Name="password" Placeholder="Password" IsPassword="True" HorizontalTextAlignment="Center" FontSize="36"></Entry>
        <Button Text="Login" Clicked="Button_Clicked"></Button>
    </StackLayout>
</ContentPage>